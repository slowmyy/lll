{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "expo start --tunnel",
    "start": "expo start --tunnel",
    "start:clean": "npm run clean:cache && expo start --clear --tunnel",
    "start:local": "expo start --localhost",
    "start:lan": "expo start",
    "tunnel": "expo start --tunnel",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean:cache": "rm -rf node_modules/.cache .expo .metro",
    "check:port": "lsof -i :8081 || echo 'Port 8081 is free'",
    "free:port": "node free-port-8081.js",
    "check:versions": "node -v && npm -v && npx expo --version",
    "check:env": "node check-env.js",
    "check:health": "node health-check.js",
    "doctor": "npx expo-doctor",
    "prestart": "node check-env.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@fal-ai/client": "^1.3.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "~54.0.0",
    "expo-apple-authentication": "~8.0.7",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.10",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "lucide-react-native": "^0.468.0",
    "nativewind": "^4.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-worklets-core": "^1.3.3",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-video": "^6.16.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.11",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=20.19.4",
    "npm": ">=10.0.0"
  }
}
